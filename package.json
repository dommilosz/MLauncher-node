{
  "name": "mlauncher",
  "version": "1.7.0",
  "productName": "MLauncher",
  "description": "Modded Minecraft Launcher",
  "license": "UNLICENSED",
  "homepage": "https://github.com/dommilosz/Mlauncher-node",
  "bugs": {
    "url": "https://github.com/dommilosz/Mlauncher-node/issues"
  },
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "cilinux": "node build.js WINDOWS && node build.js LINUX",
    "cidarwin": "node build.js MAC",
    "dist": "cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true node build.js",
    "dist:win": "npm run dist -- WINDOWS",
    "dist:mac": "npm run dist -- MAC",
    "dist:linux": "npm run dist -- LINUX",
    "lint": "eslint --config .eslintrc.json .",
    "debug": "electron --inspect=5858 ."
  },
  "engines": {
    "node": "12.x.x"
  },
  "dependencies": {
    "adm-zip": "^0.4.14",
    "async": "^3.2.0",
    "discord-rpc": "3.1.0",
    "ejs": "^3.0.1",
    "ejs-electron": "^2.0.3",
    "electron-updater": "^4.2.4",
    "fs-extra": "^8.1.0",
    "github-syntax-dark": "^0.5.0",
    "jquery": "^3.5.0",
    "minecraft-launcher-core": "^3.13.3",
    "request": "^2.88.2",
    "semver": "^7.1.3",
    "tar-fs": "^2.0.0",
    "winreg": "^1.2.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.0",
    "eslint": "^6.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dommilosz/Mlauncher-node.git"
  }
